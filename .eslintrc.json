{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "no-unused-expressions":"off",
      "no-plusplus":"off",
      "consistent-return":"off",
      "no-console":"off"
    }
}
